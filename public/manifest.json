{
    "theme_color": "#0e0e0e",
    "background_color": "#040404",
    "display": "standalone",
    "scope": "/",
    "start_url": "/",
    "name": "FlashCard",
    "short_name": "FlashCard",
    
    "description": "FlashCard Ninja",
    "icons": [
        {
            "src": "/icon-192x192.png",
            "sizes": "192x192",
            "type": "image/png"
        },
        {
            "src": "/icon-256x256.png",
            "sizes": "256x256",
            "type": "image/png"
        },
        {
            "src": "/icon-384x384.png",
            "sizes": "384x384",
            "type": "image/png"
        },
        {
            "src": "/icon-512x512.png",
            "sizes": "512x512",
            "type": "image/png"
        }
    ],
  
    "share_target": {
        "action": "/api/file-collector",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
          "title": "name",
          "text": "description",
          "url": "link",
          "files": [
            {
              "name": "lists",
              "accept": ["text/csv", ".csv"]
            },
            {
              "name": "photos",
              "accept": ["image/svg+xml", ".svg","image/jpg",".jpg", "image/png"]
            }
        
          ]
        }
      }
  }